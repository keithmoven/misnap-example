 <?xml version='1.0' encoding='utf-8'?>
<widget id="com.moven.misnap.example" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MiSnap</name>
    <description>
        A sample Apache Cordova application that uses the MiSnap plugin.
    </description>
    <author href="http://chariotsolutions.com">
        Chariot Solutions
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-media-capture" source="npm" spec="*">
        <variable name="CAMERA_USAGE_DESCRIPTION" value="App would like to access the camera." />
    </plugin>
    <access origin="*" requires-forward-secrecy="false" />
    <access origin="192.168.1.36" requires-forward-secrecy="false" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
